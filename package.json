    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.0.0",
    "postcss": "^8",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^3.4.1",
